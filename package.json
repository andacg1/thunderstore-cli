{
  "name": "@template/cli",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "license": "MIT",
  "description": "A basic Effect CLI application",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "dev": "npx tsx src/thunderstore.ts -o \"./plugins\" \"This is a test\""
  },
  "devDependencies": {
    "@effect/cli": "latest",
    "@effect/eslint-plugin": "^0.3.2",
    "@effect/language-service": "^0.17.1",
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@effect/vitest": "latest",
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@types/decompress": "^4.2.7",
    "@types/node": "^22.15.23",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "effect": "latest",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-codegen": "0.31.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "pnpm": {
    "patchedDependencies": {}
  },
  "dependencies": {
    "decompress": "^4.2.1",
    "yauzl": "^3.2.0",
    "zod": "^3.25.32"
  }
}
